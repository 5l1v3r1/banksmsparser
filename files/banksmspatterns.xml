            <amount group="1"><![CDATA[(\d*,?\d{1,2})\s[\w]*;\sОстаток]]></amount>
            <detail group="1"><![CDATA[\:\d{2};\s(.*);\s\d]]></detail>
            <decimal_char optional="true">,</decimal_char>
        </pattern>
        <!--Приход по счету карты VP *9707; 73,07 RUB; Остаток: 126,85 RUB-->
        <pattern category="popolnenie">
            <match><![CDATA[Приход по счету карты.*]]></match>
            <cardId group="1"><![CDATA[VP\s\*(\d{4})]]></cardId>
            <amount group="1"><![CDATA[(\d*,?\d{1,2})\s[\w]*;\sОстаток]]></amount>
            <decimal_char optional="true">,</decimal_char>
        </pattern>
        <!--ПАО "МТС-Банк"уведомляет о переводе со счета 40817***3838 на карту 4042 XXXX XXXX 9707: 300.00 RUR; Остаток: 16,500.00 RUR; 06.01.2018 17:25-->
        <pattern category="perevod">
            <match><![CDATA[.*о переводе со счета.*]]></match>
            <cardId group="1"><![CDATA[со счета ([\d\*]*)]]></cardId>
            <amount group="1"><![CDATA[([\d\,]*.?\d{1,2})\s[\w]*;\sОстаток]]></amount>
            <decimal_group_char optional="true">,</decimal_group_char>
        </pattern>
    </patterns>
